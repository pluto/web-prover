{
	"mode": "Origo",
	"notary_host": "localhost",
	"notary_port": 7443,
	"target_method": "GET",
	"target_url": "https://localhost:8085/health",
	"target_headers": {},
	"target_body": "",
	"max_sent_data": 10000,
	"max_recv_data": 10000,
	"proving": {
		"witnesses": [
			{
				"val": []
			}
		],
		"serialized_pp": [],
		"manifest": {
			"manifestVersion": "1",
			"id": "reddit-user-karma",
			"title": "Total Reddit Karma",
			"description": "Generate a proof that you have a certain amount of karma",
			"prepareUrl": "https://www.reddit.com/login/",
			"request": {
				"method": "GET",
				"version": "HTTP/1.1",
				"url": "https://localhost:8085/health",
				"headers": {
					"accept-encoding": "identity"
				},
				"body": {
					"userId": "<% userId %>"
				},
				"vars": {
					"userId": {
						"regex": "[a-z]{,20}+"
					},
					"token": {
						"type": "base64",
						"length": 32
					}
				}
			},
			"response": {
				"status": "200",
				"version": "HTTP/1.1",
				"message": "OK",
				"headers": {
					"Content-Type": "application/json"
				},
				"body": {
					"json": [
						"data",
						"items",
						0
					],
					"contains": "this_string_exists_in_body"
				}
			}
		}
	}
}